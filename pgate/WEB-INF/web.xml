<web-app xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
    version="2.4">

  <display-name>Payment Gateway App</display-name>
  <description>
	Payment Gateway App
  </description>


    <servlet>
        <servlet-name>BillDeskServlet</servlet-name>
        <servlet-class>BillDeskServlet</servlet-class>
    </servlet>

	<servlet>
        <servlet-name>EServeServlet</servlet-name>
        <servlet-class>EServeServlet</servlet-class>
    </servlet>

	<servlet>
        <servlet-name>ICICIServlet</servlet-name>
        <servlet-class>ICICIServlet</servlet-class>
    </servlet>

	<servlet>
        <servlet-name>responseServlet</servlet-name>
        <servlet-class>responseServlet</servlet-class>
    </servlet>

	<servlet>
        <servlet-name>pgServlet</servlet-name>
        <servlet-class>pgServlet</servlet-class>
    </servlet>

	<servlet>
        <servlet-name>payServlet</servlet-name>
        <servlet-class>payServlet</servlet-class>
    </servlet>

	<servlet>
        <servlet-name>txnDetails</servlet-name>
        <servlet-class>txnDetails</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>OfflineServlet</servlet-name>
        <servlet-class>OfflineServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>OfflineResponse</servlet-name>
        <servlet-class>OfflineResponse</servlet-class>
    </servlet>
    

<!-- Admin Servlet Mapping Start-->
    <servlet>
        <servlet-name>AdminLoginServlet</servlet-name>
        <servlet-class>AdminLoginServlet</servlet-class>
    </servlet>
    
       <servlet>
        <servlet-name>AdminLogoutServlet</servlet-name>
        <servlet-class>AdminLogoutServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>TransactionServlet</servlet-name>
        <servlet-class>admin.servlet.TransactionServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>MidClientPgServlet</servlet-name>
        <servlet-class>admin.servlet.MidClientPgServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>GetMids</servlet-name>
        <servlet-class>admin.servlet.GetMids</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>ViewMidClientPg</servlet-name>
        <servlet-class>admin.servlet.ViewMidClientPg</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>GetTxnData</servlet-name>
        <servlet-class>admin.servlet.GetTxnData</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>ViewPgMaster</servlet-name>
        <servlet-class>admin.servlet.ViewPgMaster</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>PgMasterServlet</servlet-name>
        <servlet-class>admin.servlet.PgMasterServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>ViewCurrencyMaster</servlet-name>
        <servlet-class>admin.servlet.ViewCurrencyMaster</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>CurrencyMasterServlet</servlet-name>
        <servlet-class>admin.servlet.CurrencyMasterServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>ViewClientMaster</servlet-name>
        <servlet-class>admin.servlet.ViewClientMaster</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>ClientMasterServlet</servlet-name>
        <servlet-class>admin.servlet.ClientMasterServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>ViewMerchantMaster</servlet-name>
        <servlet-class>admin.servlet.ViewMerchantMaster</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>MerchantMasterServlet</servlet-name>
        <servlet-class>admin.servlet.MerchantMasterServlet</servlet-class>
    </servlet>
    
<!-- Admin Servlet Mapping End-->    

   <servlet-mapping>
        <servlet-name>BillDeskServlet</servlet-name>
        <url-pattern>/BillDeskServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>EServeServlet</servlet-name>
        <url-pattern>/EServeServlet</url-pattern>
    </servlet-mapping>

	<servlet-mapping>
        <servlet-name>ICICIServlet</servlet-name>
        <url-pattern>/ICICIServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>responseServlet</servlet-name>
        <url-pattern>/responseServlet</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>pgServlet</servlet-name>
        <url-pattern>/pgServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>payServlet</servlet-name>
        <url-pattern>/payServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>txnDetails</servlet-name>
        <url-pattern>/transactionDetails</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>OfflineServlet</servlet-name>
        <url-pattern>/OfflineServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>OfflineResponse</servlet-name>
        <url-pattern>/OfflineResponse</url-pattern>
    </servlet-mapping>
    
    

<!-- Admin URL Start -->
    <servlet-mapping>
        <servlet-name>AdminLoginServlet</servlet-name>
        <url-pattern>/admin/Login.do</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>AdminLogoutServlet</servlet-name>
        <url-pattern>/admin/Logout.do</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>TransactionServlet</servlet-name>
        <url-pattern>/admin/TransactionServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>MidClientPgServlet</servlet-name>
        <url-pattern>/admin/MidClientPgServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>GetMids</servlet-name>
        <url-pattern>/admin/GetMids</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ViewMidClientPg</servlet-name>
        <url-pattern>/admin/ViewMidClientPg</url-pattern>
    </servlet-mapping>
    
     <servlet-mapping>
        <servlet-name>GetTxnData</servlet-name>
        <url-pattern>/admin/GetTxnData</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ViewPgMaster</servlet-name>
        <url-pattern>/admin/ViewPgMaster</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>PgMasterServlet</servlet-name>
        <url-pattern>/admin/PgMasterServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ViewCurrencyMaster</servlet-name>
        <url-pattern>/admin/ViewCurrencyMaster</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>CurrencyMasterServlet</servlet-name>
        <url-pattern>/admin/CurrencyMasterServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ViewClientMaster</servlet-name>
        <url-pattern>/admin/ViewClientMaster</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ClientMasterServlet</servlet-name>
        <url-pattern>/admin/ClientMasterServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ViewMerchantMaster</servlet-name>
        <url-pattern>/admin/ViewMerchantMaster</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>MerchantMasterServlet</servlet-name>
        <url-pattern>/admin/MerchantMasterServlet</url-pattern>
    </servlet-mapping>
    
<!-- Admin URL End -->

    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>



  <resource-ref>
    <description>DB Connection</description>
      <res-ref-name>jdbc/MySQLDB</res-ref-name>
      <res-type>javax.sql.DataSource</res-type>
      <res-auth>Container</res-auth>
  </resource-ref>

  <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
		<welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
  </welcome-file-list>

</web-app>

